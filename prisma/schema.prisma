generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./waitlist.db"
}

model Waitlist {
  id        Int      @id @default(autoincrement())
  fullname  String
  email     String   @unique
  phone     String?
  createdAt DateTime @default(now())
}
